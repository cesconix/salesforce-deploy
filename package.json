{
  "name": "sfdc-utils",
  "version": "1.0.0-alpha.0",
  "description": "Utils for Salesforce Commerce Cloud",
  "main": "dist/sfdc-utils.cjs.js",
  "unpkg": "dist/sfdc-utils.umd.js",
  "module": "dist/sfdc-utils.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c",
    "prepare": "npm run clean && standard && npm test"
  },
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-node-resolve": "^4.0.0",
    "standard": "^12.0.1",
    "jest": "^23.6.0"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.js)$"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesconix/sfdc-utils.git"
  },
  "keywords": [
    "salesforce",
    "demandware",
    "webdav",
    "cartridge",
    "upload"
  ],
  "author": "Francesco Pasqua <cesconix@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesconix/sfdc-utils/issues"
  },
  "homepage": "https://github.com/cesconix/sfdc-utils#readme"
}
