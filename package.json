{
  "name": "sfdc-utils",
  "version": "1.0.0-alpha.1",
  "description": "Utils for Salesforce Commerce Cloud",
  "main": "dist/sfdc-utils.cjs.js",
  "unpkg": "dist/sfdc-utils.umd.js",
  "module": "dist/sfdc-utils.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage --coverageReporters=text-lcov | coveralls",
    "build": "rollup -c",
    "start": "babel-node src/main"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/node": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "^3.0.0",
    "standard": "^12.0.1"
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec\\.js)$"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesconix/sfdc-utils.git"
  },
  "keywords": [
    "salesforce",
    "demandware",
    "webdav",
    "cartridge",
    "upload"
  ],
  "author": "Francesco Pasqua <cesconix@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesconix/sfdc-utils/issues"
  },
  "homepage": "https://github.com/cesconix/sfdc-utils#readme"
}
